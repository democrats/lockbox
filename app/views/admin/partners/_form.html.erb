<% form_for @partner, :url => [@partner] do |f| %>
  <%= f.text_field :name %>
  <% unless @partner.new_record? %>
  API key: <%= @partner.api_key %>
  <% end %>
  <%= f.text_field :phone_number %>
  <%= f.text_field :email %>
  <%= f.text_field :organization %>
  <%= f.text_field :max_requests, :label => "Max Requests (per hour)" %>
  <%= f.submit 'Save' %>
<% end %>